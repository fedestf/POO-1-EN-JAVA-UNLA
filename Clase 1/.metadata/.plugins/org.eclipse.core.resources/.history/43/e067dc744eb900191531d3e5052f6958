package modelo;


public class Numero {
	
	private int n;

	public Numero(int n) {
		super();
		this.n = n;
	}

	public int getN() {
		return n;
	}

	public void setN(int n) {
		this.n = n;
	}
	
	public int Sumar(Numero n) {

		return (this.n+n.getN());
	}
	
	public int Sumar2(Numero n,Numero n1) {

		return (n.getN()+n1.getN());
	}
	public int Multiplicar (Numero n) {
		return (this.n*n.getN());
	}
	
	public boolean EsPar(Numero n) {
		if(this.n%n.getN()!=1) {
			return false;
		}
		else {
			return true;
		}
	}
}




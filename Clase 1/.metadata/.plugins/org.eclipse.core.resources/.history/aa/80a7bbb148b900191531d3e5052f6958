package modelo;


public class Numero {
	
	private int n;

	public Numero(int n) {
		super();
		this.n = n;
	}

	public int getN() {
		return n;
	}

	public void setN(int n) {
		this.n = n;
	}
	
	public int Sumar(Numero n1) {

		return (n1.getN()+n1.getN());
	}

	public int Multiplicar (Numero n) {
		return (n.getN()*n.getN());
	}
}




package testeo;
import modelo.*;
public class Test {

	public static void main(String[] args) {
		// Creacion de numeros
		Numero numero1=new Numero (13);
		Numero numero2=new Numero (10);
		//flag de carga correcta
		System.out.println(numero2.getN()+" y "+numero1.getN());
	//suma
	
	System.out.println(numero1.Sumar(numero2));
	
	 System.out.println(numero1.Sumar2(numero1, numero2));
	
	//multiplicacion
	System.out.println(numero1.Multiplicar(numero2));
	
	// es par
	
	System.out.println(numero1.EsPar(numero1));
	System.out.println(numero2.EsPar(numero2));
	// es primo
	System.out.println(numero1.EsPrimo(numero1));
	
	}
	

}
